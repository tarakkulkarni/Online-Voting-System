<%- include('partials/header') %>
<h1 class="main-heading">Result</h1>


<% results.forEach((ele) => { %>
   <div class="show-results">
    <ul class="list-group">
        <li class="list-group-item active">Election Name <%= ele.election_name %></li>
        <li class="show-election-row list-group-item">Candidate Name <%= ele.candidate_name %></li>
        <li class="show-election-row list-group-item">votes <%= ele.votes %></li>
      </ul>
  </div>
<% }); %>

<%- include('partials/footer') %>
